@model EstudiantesApp.Transporte.EstudianteDto

@{
    ViewData["Title"] = "Details";
}

<h2 class="text-info">Detalle del estudiante</h2>

<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Apellido)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaInscripcion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaInscripcion)
        </dd>
    </dl>
</div>
<div class="form-group">
    <div class="row">
       <div class="col-sm-2">
          @Html.ActionLink("Editar", "Edit", new {id=Model.Id}, new{@class="btn btn-primary form-control"})
          
       </div>
        <div class="col-sm-2">
            <a asp-action="Index" class="btn btn-success form-control">Volver a la Lista</a>
        </div>
    </div>
   
    
</div>
