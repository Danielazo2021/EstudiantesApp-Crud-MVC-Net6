@model EstudiantesApp.Transporte.EstudianteDto

@{
    ViewData["Title"] = "Delete";
}


<h2 cllass=text-info>Eliminar estudiante</h2>
<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Apellido)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaInscripcion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaInscripcion)
        </dd>
       
    </dl>
    
    <form asp-action="Delete">
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <input type="submit" value="Eliminar" class="btn btn-danger form-control" /> 
                </div>
                <div class="col-sm-2">
                    <a asp-action="Index" class="btn btn-success form-control">Volver a la lista</a>
                </div>
            </div>
        </div>
      <!--  <input type="hidden" asp-for="Id" />
         -->
      
    </form>
</div>
